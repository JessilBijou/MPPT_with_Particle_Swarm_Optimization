import numpy as np
import matplotlib.pyplot as plt

# Sample data (voltage vs. current)
voltage_data = [1.2, 2.0, 2.5, 3.0, 3.5]
current_data = [0.5, 1.2, 1.8, 2.2, 2.5]

# Plot the I-V curve
plt.figure()
plt.plot(voltage_data, current_data, 'ro-')
plt.xlabel('Voltage (V)')
plt.ylabel('Current (A)')
plt.title('Solar Panel I-V Characteristic')
plt.grid()
plt.show()
